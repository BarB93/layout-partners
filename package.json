{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint src/",
    "fix": "eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ],
        "extends": [
          "react-app",
          "react-app/jest"
        ],
        "rules": {
          "no-console": "warn",
          "no-unused-vars": "warn",
          "no-debugger": "warn",
          "jsx-quotes": [
            "error",
            "prefer-single"
          ],
          "quotes": [
            "error",
            "single"
          ],
          "semi": [
            "error",
            "never"
          ],
          "react/prop-types": "off",
          "react-hooks/exhaustive-deps": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.5%",
    "ie >= 11"
  ],
  "devDependencies": {
    "sass": "^1.44.0"
  }
}
